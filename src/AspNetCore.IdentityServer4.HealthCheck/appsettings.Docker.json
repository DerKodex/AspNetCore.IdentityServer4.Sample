{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthChecks": {
    "Service": {
      "Redis": {
        "Host": "idsrv-redis",
        "Port": 6379
      },
      "IdentityServer": {
        "Host": "idsrv-auth",
        "Port": 6001
      }
    },
    "Endpoints": [
      {
        "Name": "Backedn",
        "Url": "https://host.docker.internal:5001/Health"
      },
      {
        "Name": "Auth",
        "Url": "https://host.docker.internal:6001/Health"
      },
      {
        "Name": "Services",
        "Url": "https://host.docker.internal:7001/Health"
      }
    ]
  }
}
