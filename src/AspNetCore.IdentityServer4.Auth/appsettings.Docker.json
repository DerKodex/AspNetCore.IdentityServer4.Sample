{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "LdapServer": {
    "Url": "openldap",
    "Port": 389,
    "Ssl": false,
    "BindDn": "cn=admin,dc=example,dc=org",
    "BindCredentials": "admin",
    "SearchBase": "dc=example,dc=org",
    "SearchFilter": "(&(objectClass=person)(uid={0}))"
  },
  "Host": {
    "Redis": "redis:6379"
  },
  "Global": {
    "SigningCredential": {
      "DefaultExpiry": {
        "Second": 120
      }
    }
  }
}
